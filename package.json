{
  "name": "meetup-web-components",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "description": "Meetup Web UI Component Library",
  "engine": "node >=6.5.0",
  "scripts": {
    "generate": "node utils/generate.js",
    "lint": "eslint . --ext .js,.jsx --fix",
