    "react-intl": "2.1.3",
    "react-router": "2.6.1",
    "react-transform-catch-errors": "1.0.2",
    "sass-loader": "4.0.2",
    "sassquatch2": "2.3.0",
    "style-loader": "0.13.1",
    "webpack": "2.1.0-beta.15",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-node-externals": "1.2.0",
    "webpack-svgstore-plugin": "3.0.3"
  },
  "devDependencies": {
    "@kadira/storybook": "2.2.2",
    "inquirer": "1.2.2",
    "jest-cli": "15.1.1",
    "npm-run-all": "2.3.0"
