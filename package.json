    "test:unit": "jest --silent --config jest.config.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meetup/meetup-web-components.git"
  },
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-jest": "15.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-intl": "2.1.3",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.11.0",
    "babel-register": "6.11.6",
    "bourbon": "4.2.3",
    "classnames": "2.2.5",
    "css-loader": "0.23.1",
    "eslint": "3.2.2",
    "eslint-loader": "1.5.0",
    "eslint-plugin-react": "6.0.0",
    "julep": "1.4.4",
    "meetup-swatches": "3.0.1",
    "meetup-web-platform": "0.4.129",
