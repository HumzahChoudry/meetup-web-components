{
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}",
    "!**/{node_modules,lib,coverage,assets,icons}/**"
  ],
  "coverageDirectory": "<rootDir>/coverage",
  "setupFiles": [
    "<rootDir>/src/utils/jestSystemShim.js"
  ],
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "preprocessorIgnorePatterns": [
    "node_modules(?!\/meetup-web-platform)"
  ],
  "testRegex": "src/.*\\.test\\.jsx?$"
}
